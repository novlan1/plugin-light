    "enquirer": "^2.4.1",
    "eslint": "^8.22.0",
    "eslint-config-light": "^1.0.6",
    "glob": "^10.3.10",
    "glob-to-regexp": "^0.4.1",
    "htmlparser2": "^8.0.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "lint-staged": "^12.5.0",
