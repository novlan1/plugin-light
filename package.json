    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jest": "^29.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.22.0",
    "eslint-config-light": "^1.0.6",
    "glob-to-regexp": "^0.4.1",
